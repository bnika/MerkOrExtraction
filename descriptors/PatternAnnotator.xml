</inputs>
<outputs>
<type allAnnotatorFeatures="true">is.merkor.relationextraction.types.Pattern</type>
</outputs>
<languagesSupported>
<language>en</language>
</languagesSupported>
</capability>
</capabilities>
<operationalProperties>
